    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "preversion": "npm test"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.12.6",
